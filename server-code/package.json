{
  "name": "server-code",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --inspect=5858 --experimental-modules -r ts-node/register ./src/app.ts",
    "start:watch": "nodemon --legacy-watch",
    "build": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-ws": "^4.0.0",
    "morgan": "~1.9.1",
    "node-schedule": "^1.3.2",
    "rethinkdb": "^2.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-ws": "^3.0.0",
    "@types/node": "^12.0.0",
    "@types/rethinkdb": "^2.3.14",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      "public"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
